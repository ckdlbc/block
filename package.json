{
  "name": "block",
  "version": "2.3.5",
  "description": "专为 Axios.js 开发的模块API管理模式。它采用集中式存储管理应用的所有 API 模块，并以相应的规则保证 API 以一种统一的方式进行调用。现已支持Vuex Plugin",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clear": "rm -rf dist",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:publish": "npm run docs:build && def-deploy --path ./docs/dist",
    "build": "npm run clear && tsc",
    "watch": "tsc -w",
    "test": "jest --forceExit --coverage --verbose",
    "test:watch": "npm run test -- --watchAll",
    "prepublish": "npm run build && npm run docs:publish"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ckdlbc/block.git"
  },
  "author": "Arvinsu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ckdlbc/block/issues"
  },
  "homepage": "https://github.com/ckdlbc/block",
  "dependencies": {
    "@def/oss-deploy": "^0.0.5",
    "@types/axios": "^0.14.0",
    "@types/jest": "^23.1.3",
    "@types/node": "^10.5.1",
    "@types/qs": "^6.5.1",
    "@types/url-template": "^2.0.28",
    "axios": "^0.18.0",
    "qs": "^6.5.2",
    "tslib": "^1.9.3",
    "tslint-config-standard": "^7.1.0",
    "url-template": "^2.0.8",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "build": "^0.1.4",
    "jest": "^23.2.0",
    "ts-jest": "^23.0.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1",
    "vuepress": "^0.14.2"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  }
}
